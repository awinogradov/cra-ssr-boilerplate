{
  "name": "cra-ssr-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "compression": "1.7.3",
    "config": "2.0.1",
    "express": "4.16.3",
    "ignore-styles": "5.0.1",
    "react": "16.5.1",
    "react-dom": "16.5.1"
  },
  "scripts": {
    "start": "NODE_CONFIG_DIR=.config NODE_ENV=development ts-node-dev server",
    "start:production": "NODE_CONFIG_DIR=.config NODE_ENV=production node dist/server",
    "build": "npm run build:clean && npm run build:compile && npm run build:static",
    "build:clean": "rm -r dist",
    "build:compile": "tsc",
    "build:static": "react-app-rewired build --config-overrides ./.config/react-scripts.js"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
    "@types/express": "4.16.0",
    "@types/node": "10.9.4",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "cpx": "1.5.0",
    "react-app-rewire-typescript": "2.0.2",
    "react-app-rewire-webpack-bundle-analyzer": "1.0.1",
    "react-app-rewired": "1.6.2",
    "react-scripts": "1.1.5",
    "ts-node": "7.0.1",
    "ts-node-dev": "1.0.0-pre.30",
    "typescript": "3.0.3"
  },
  "proxy": {
    "/_": {
      "target": "http://localhost:3001/",
      "ws": true
    }
  }
}
