{
  "name": "cra-ssr-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack --config .config/webpack.server.js --watch",
    "start:production": "NODE_ENV=production node dist/server",
    "start:docker": "docker run -d --name cra-ssr-cs-hmr -p 3000:3000 cra-ssr-cs-hmr:latest",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "build:clean": "rm -r dist",
    "build:server": "NODE_ENV=production webpack --config .config/webpack.server.js",
    "build:client": "react-app-rewired build --config-overrides ./.config/webpack.client.js",
    "build:docker": "docker build -t cra-ssr-cs-hmr .",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "1.18.3",
